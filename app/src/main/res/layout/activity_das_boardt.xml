<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DasBoardt">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="210dp"
        android:layout_height="137dp"
        android:layout_marginTop="62dp"
        android:layout_marginBottom="47dp"
        app:layout_constraintBottom_toBottomOf="@+id/button1"
        app:layout_constraintEnd_toStartOf="@+id/btn_sugerencias"
        app:layout_constraintStart_toStartOf="@+id/btn_sugerencias"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/user" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginBottom="198dp"
        android:text="Usuario Principal"
        app:layout_constraintBottom_toTopOf="@+id/button1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_volver_dash1" />

    <Button
        android:id="@+id/btn_sugerencias"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="79dp"
        android:text="Sugerencias"
        app:layout_constraintBottom_toTopOf="@+id/btn_ver_datos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button1"
        app:layout_constraintTop_toBottomOf="@+id/button1" />

    <Button
        android:id="@+id/button1"
        android:layout_width="163dp"
        android:layout_height="90dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="15dp"
        android:text="Gastos"
        app:layout_constraintBottom_toTopOf="@+id/btn_sugerencias"
        app:layout_constraintEnd_toStartOf="@+id/btn_sugerencias"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/btn_volver_dash1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:layout_marginBottom="61dp"
        android:text="Cerrrar Session"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_ver_datos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="149dp"
        android:text="Ver Datos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/btn_sugerencias"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/btn_sugerencias" />


</androidx.constraintlayout.widget.ConstraintLayout>